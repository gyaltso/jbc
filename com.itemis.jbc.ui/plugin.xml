<?xml version="1.0" encoding="windows-1252"?>
<?eclipse version="3.0"?>
<plugin>
	<extension
		point="org.eclipse.ui.editors">
		<editor
        class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:com.itemis.jbc.ui.custom.JBCEditor"
        contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
        default="true"
        extensions="jbc,class"
        id="com.itemis.jbc.JBC"
        name="JBC Editor">
     <contentTypeBinding
           contentTypeId="org.eclipse.jdt.core.javaClass">
     </contentTypeBinding>
		</editor>
	</extension>
	<extension
		point="org.eclipse.ui.handlers">
		<handler
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclarationHandler"
			commandId="org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclaration">
			<activeWhen>
				<reference
					definitionId="com.itemis.jbc.JBC.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
		<handler
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.editor.handler.ValidateActionHandler"
			commandId="com.itemis.jbc.JBC.validate">
		<activeWhen>
			<reference
					definitionId="com.itemis.jbc.JBC.Editor.opened">
			</reference>
		</activeWhen>
		</handler>
		<!-- copy qualified name -->
		<handler
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedNameHandler"
			commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName">
			<activeWhen>
				<reference definitionId="com.itemis.jbc.JBC.Editor.opened" />
			</activeWhen>
		</handler>
		<handler
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedNameHandler"
			commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName">
			<activeWhen>
				<and>
					<reference definitionId="com.itemis.jbc.JBC.XtextEditor.opened" />
					<iterate>
						<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
					</iterate>
				</and>
			</activeWhen>
		</handler>
	</extension>
	<extension point="org.eclipse.core.expressions.definitions">
		<definition id="com.itemis.jbc.JBC.Editor.opened">
			<and>
				<reference definitionId="isActiveEditorAnInstanceOfXtextEditor"/>
				<with variable="activeEditor">
					<test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName"
						value="com.itemis.jbc.JBC"
						forcePluginActivation="true"/>
				</with>
			</and>
		</definition>
		<definition id="com.itemis.jbc.JBC.XtextEditor.opened">
			<and>
				<reference definitionId="isXtextEditorActive"/>
				<with variable="activeEditor">
					<test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName"
						value="com.itemis.jbc.JBC"
						forcePluginActivation="true"/>
				</with>
			</and>
		</definition>
	</extension>
	<extension
			point="org.eclipse.ui.preferencePages">
		<page
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
			id="com.itemis.jbc.JBC"
			name="JBC">
			<keywordReference id="com.itemis.jbc.ui.keyword_JBC"/>
		</page>
		<page
			category="com.itemis.jbc.JBC"
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.editor.syntaxcoloring.SyntaxColoringPreferencePage"
			id="com.itemis.jbc.JBC.coloring"
			name="Syntax Coloring">
			<keywordReference id="com.itemis.jbc.ui.keyword_JBC"/>
		</page>
		<page
			category="com.itemis.jbc.JBC"
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.editor.templates.XtextTemplatePreferencePage"
			id="com.itemis.jbc.JBC.templates"
			name="Templates">
			<keywordReference id="com.itemis.jbc.ui.keyword_JBC"/>
		</page>
	</extension>
	<extension
			point="org.eclipse.ui.propertyPages">
		<page
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
			id="com.itemis.jbc.JBC"
			name="JBC">
			<keywordReference id="com.itemis.jbc.ui.keyword_JBC"/>
			<enabledWhen>
				<adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
			<filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
		</page>
	</extension>
	<extension
		point="org.eclipse.ui.keywords">
		<keyword
			id="com.itemis.jbc.ui.keyword_JBC"
			label="JBC"/>
	</extension>
	<extension
		point="org.eclipse.ui.commands">
	<command
			description="Trigger expensive validation"
			id="com.itemis.jbc.JBC.validate"
			name="Validate">
	</command>
	<!-- copy qualified name -->
	<command
			id="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
			categoryId="org.eclipse.ui.category.edit"
			description="Copy the qualified name for the selected element"
			name="Copy Qualified Name">
	</command>
	<command
			id="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName"
			categoryId="org.eclipse.ui.category.edit"
			description="Copy the qualified name for the selected element"
			name="Copy Qualified Name">
	</command>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.edit">
			 <command
				 commandId="com.itemis.jbc.JBC.validate"
				 style="push"
				 tooltip="Trigger expensive validation">
			<visibleWhen checkEnabled="false">
				<reference
					definitionId="com.itemis.jbc.JBC.Editor.opened">
				</reference>
			</visibleWhen>
		</command>
		</menuContribution>
		<!-- copy qualified name -->
		<menuContribution locationURI="popup:#TextEditorContext?after=copy">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<reference definitionId="com.itemis.jbc.JBC.Editor.opened" />
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution locationURI="menu:edit?after=copy">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<reference definitionId="com.itemis.jbc.JBC.Editor.opened" />
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution locationURI="popup:org.eclipse.xtext.ui.outline?after=additions">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName"
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<and>
						<reference definitionId="com.itemis.jbc.JBC.XtextEditor.opened" />
						<iterate>
							<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
						</iterate>
					</and>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution locationURI="popup:#TextEditorContext?endof=group.find">
			<command commandId="org.eclipse.xtext.ui.editor.FindReferences">
				<visibleWhen checkEnabled="false">
					<reference definitionId="com.itemis.jbc.JBC.Editor.opened">
					</reference>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.handlers">
		<handler
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.editor.findrefs.FindReferencesHandler"
			commandId="org.eclipse.xtext.ui.editor.FindReferences">
			<activeWhen>
				<reference
					definitionId="com.itemis.jbc.JBC.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
	<!-- adding resource factories -->
	<extension
		point="org.eclipse.emf.ecore.extension_parser">
		<parser
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.resource.IResourceFactory"
			type="jbc">
		</parser>
	</extension>
	<extension point="org.eclipse.xtext.extension_resourceServiceProvider">
		<resourceServiceProvider
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.resource.IResourceUIServiceProvider"
			uriExtension="jbc">
		</resourceServiceProvider>
	</extension>
	<extension
		point="org.eclipse.emf.ecore.extension_parser">
		<parser
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.resource.IResourceFactory"
			type="class">
		</parser>
	</extension>
	<extension point="org.eclipse.xtext.extension_resourceServiceProvider">
		<resourceServiceProvider
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.resource.IResourceUIServiceProvider"
			uriExtension="class">
		</resourceServiceProvider>
	</extension>
	<!-- marker definitions for com.itemis.jbc.JBC -->
	<extension
			id="jbc.check.fast"
			name="JBC Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.fast"/>
		<persistent value="true"/>
	</extension>
	<extension
			id="jbc.check.normal"
			name="JBC Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.normal"/>
		<persistent value="true"/>
	</extension>
	<extension
			id="jbc.check.expensive"
			name="JBC Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.expensive"/>
		<persistent value="true"/>
	</extension>
	<extension point="org.eclipse.xtext.builder.participant">
		<participant
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.builder.IXtextBuilderParticipant"
			fileExtensions="jbc"/>
	</extension>
	<extension point="org.eclipse.xtext.builder.participant">
		<participant
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.builder.IXtextBuilderParticipant"
			fileExtensions="class"/>
	</extension>
	<extension point="org.eclipse.ui.preferencePages">
		<page
			category="com.itemis.jbc.JBC"
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.builder.preferences.BuilderPreferencePage"
			id="com.itemis.jbc.JBC.compiler.preferencePage"
			name="Compiler">
			<keywordReference id="com.itemis.jbc.ui.keyword_JBC"/>
		</page>
	</extension>
	<extension point="org.eclipse.ui.propertyPages">
		<page
			category="com.itemis.jbc.JBC"
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.builder.preferences.BuilderPreferencePage"
			id="com.itemis.jbc.JBC.compiler.propertyPage"
			name="Compiler">
			<keywordReference id="com.itemis.jbc.ui.keyword_JBC"/>
			<enabledWhen>
				<adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
			<filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
		</page>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution locationURI="popup:#TextEditorContext?after=xtext.ui.openDeclaration">
			<command
				commandId="org.eclipse.xtext.ui.OpenGeneratedFileCommand"
				id="com.itemis.jbc.JBC.OpenGeneratedCode"
				style="push">
				<visibleWhen checkEnabled="false">
					<reference definitionId="com.itemis.jbc.JBC.Editor.opened" />
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.handlers">
		<handler
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.generator.trace.OpenGeneratedFileHandler"
			commandId="org.eclipse.xtext.ui.OpenGeneratedFileCommand">
			<activeWhen>
				<reference definitionId="com.itemis.jbc.JBC.Editor.opened" />
			</activeWhen>
		</handler>
	</extension>
	<!-- Quick Outline -->
	<extension
		point="org.eclipse.ui.handlers">
		<handler 
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.editor.outline.quickoutline.ShowQuickOutlineActionHandler"
			commandId="org.eclipse.xtext.ui.editor.outline.QuickOutline">
			<activeWhen>
				<reference
					definitionId="com.itemis.jbc.JBC.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
	<extension
		point="org.eclipse.ui.commands">
		<command
			description="Open the quick outline."
			id="org.eclipse.xtext.ui.editor.outline.QuickOutline"
			name="Quick Outline">
		</command>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.open">
			<command commandId="org.eclipse.xtext.ui.editor.outline.QuickOutline"
				style="push"
				tooltip="Open Quick Outline">
				<visibleWhen checkEnabled="false">
					<reference definitionId="com.itemis.jbc.JBC.Editor.opened"/>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<!-- quickfix marker resolution generator for com.itemis.jbc.JBC -->
	<extension
			point="org.eclipse.ui.ide.markerResolution">
		<markerResolutionGenerator
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="com.itemis.jbc.ui.jbc.check.fast">
			<attribute
				name="FIXABLE_KEY"
				value="true">
			</attribute>
		</markerResolutionGenerator>
		<markerResolutionGenerator
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="com.itemis.jbc.ui.jbc.check.normal">
			<attribute
				name="FIXABLE_KEY"
				value="true">
			</attribute>
		</markerResolutionGenerator>
		<markerResolutionGenerator
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="com.itemis.jbc.ui.jbc.check.expensive">
			<attribute
				name="FIXABLE_KEY"
				value="true">
			</attribute>
		</markerResolutionGenerator>
	</extension>
	<!-- Rename Refactoring -->
	<extension point="org.eclipse.ui.handlers">
		<handler 
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.refactoring.ui.DefaultRenameElementHandler"
			commandId="org.eclipse.xtext.ui.refactoring.RenameElement">
			<activeWhen>
				<reference
					definitionId="com.itemis.jbc.JBC.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.edit">
			<command commandId="org.eclipse.xtext.ui.refactoring.RenameElement"
				style="push">
				<visibleWhen checkEnabled="false">
					<reference
						definitionId="com.itemis.jbc.JBC.Editor.opened">
					</reference>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.preferencePages">
		<page
			category="com.itemis.jbc.JBC"
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.refactoring.ui.RefactoringPreferencePage"
			id="com.itemis.jbc.JBC.refactoring"
			name="Refactoring">
			<keywordReference id="com.itemis.jbc.ui.keyword_JBC"/>
		</page>
	</extension>
	<extension point="org.eclipse.compare.contentViewers">
		<viewer id="com.itemis.jbc.JBC.compare.contentViewers"
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.compare.InjectableViewerCreator"
			extensions="jbc,class">
		</viewer>
	</extension>
	<extension point="org.eclipse.compare.contentMergeViewers">
		<viewer id="com.itemis.jbc.JBC.compare.contentMergeViewers"
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.compare.InjectableViewerCreator"
			extensions="jbc,class" label="JBC Compare">
		</viewer>
	</extension>
	<extension point="org.eclipse.ui.editors.documentProviders">
		<provider id="com.itemis.jbc.JBC.editors.documentProviders"
			class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:org.eclipse.xtext.ui.editor.model.XtextDocumentProvider"
			extensions="jbc,class">
		</provider>
	</extension>
	<extension point="org.eclipse.team.core.fileTypes">
		<fileTypes
			extension="jbc"
			type="text">
		</fileTypes>
		<fileTypes
			extension="class"
			type="text">
		</fileTypes>
	</extension>
 <extension
       point="org.eclipse.ui.menus">
    <menuContribution
          allPopups="false"
          locationURI="popup:org.eclipse.ui.navigator.ProjectExplorer#PopupMenu?before=additions">
       <command
             commandId="com.itemis.jbc.ui.commands.CreateJBCFromClassCommand"
             label="Create .jbc"
             style="push"
             tooltip="Create a .jbc file out of the selected .class file">
          <visibleWhen
                checkEnabled="false">
             <with
                   variable="selection">
                <iterate
                      ifEmpty="false"
                      operator="and">
                   <adapt
                         type="org.eclipse.core.resources.IFile">
                      <test
                            forcePluginActivation="true"
                            property="org.eclipse.core.resources.name"
                            value="*.class">
                      </test>
                   </adapt>
                </iterate>
             </with>
          </visibleWhen>
       </command>
       <command
             commandId="com.itemis.jbc.ui.commands.CreateClassFromJBCCommand"
             label="Create .class"
             style="push"
             tooltip="Create a .class file out of the selected .jbc file">
          <visibleWhen
                checkEnabled="false">
             <with
                   variable="selection">
                <iterate
                      ifEmpty="false"
                      operator="and">
                   <adapt
                         type="org.eclipse.core.resources.IFile">
                      <test
                            forcePluginActivation="true"
                            property="org.eclipse.core.resources.name"
                            value="*.jbc">
                      </test>
                   </adapt>
                </iterate>
             </with>
          </visibleWhen>
       </command>
       <command
             commandId="com.itemis.jbc.ui.commands.OpenJBCFromJavaCommand"
             label="Open JBC"
             style="push"
             tooltip="Open the .class file created from selected .java file in JBC editor.">
          <visibleWhen
                checkEnabled="false">
             <with
                   variable="selection">
                <iterate
                      ifEmpty="false"
                      operator="and">
                   <adapt
                         type="org.eclipse.jdt.core.ICompilationUnit">
                   </adapt>
                </iterate>
             </with>
          </visibleWhen>
       </command>
    </menuContribution>
    <menuContribution
          allPopups="false"
          locationURI="popup:org.eclipse.jdt.ui.PackageExplorer?before=additions">
       <command
             commandId="com.itemis.jbc.ui.commands.CreateJBCFromClassCommand"
             label="Create .jbc"
             style="push"
             tooltip="Create a .jbc file out of the selected .class file">
          <visibleWhen
                checkEnabled="false">
             <with
                   variable="selection">
                <iterate
                      ifEmpty="false"
                      operator="and">
                   <adapt
                         type="org.eclipse.core.resources.IFile">
                      <test
                            forcePluginActivation="true"
                            property="org.eclipse.core.resources.name"
                            value="*.class">
                      </test>
                   </adapt>
                </iterate>
             </with>
          </visibleWhen>
       </command>
       <command
             commandId="com.itemis.jbc.ui.commands.CreateClassFromJBCCommand"
             label="Create .class"
             style="push"
             tooltip="Create a .class file out of the selected .jbc file">
          <visibleWhen
                checkEnabled="false">
             <with
                   variable="selection">
                <iterate
                      ifEmpty="false"
                      operator="and">
                   <adapt
                         type="org.eclipse.core.resources.IFile">
                      <test
                            forcePluginActivation="true"
                            property="org.eclipse.core.resources.name"
                            value="*.jbc">
                      </test>
                   </adapt>
                </iterate>
             </with>
          </visibleWhen>
       </command>
       <command
             commandId="com.itemis.jbc.ui.commands.OpenJBCFromJavaCommand"
             label="Open JBC"
             style="push"
             tooltip="Open the .class file created from selected .java file in JBC editor.">
          <visibleWhen
                checkEnabled="false">
             <with
                   variable="selection">
                <iterate
                      ifEmpty="false"
                      operator="and">
                   <adapt
                         type="org.eclipse.jdt.core.ICompilationUnit">
                   </adapt>
                </iterate>
             </with>
          </visibleWhen>
       </command>
    </menuContribution>
 </extension>
 <extension
       point="org.eclipse.ui.commands">
    <command
          description="Create a .jbc file out of the selected .class file"
          id="com.itemis.jbc.ui.commands.CreateJBCFromClassCommand"
          name="Create .jbc">
    </command>
    <command
          description="Create a .class file out of the selected .jbc file"
