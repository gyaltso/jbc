          name="Open JBC">
    </command>
 </extension>
 <extension
       point="org.eclipse.ui.handlers">
    <handler
          class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:com.itemis.jbc.ui.commands.CreateJBCFromClassHandler"
          commandId="com.itemis.jbc.ui.commands.CreateJBCFromClassCommand">
    </handler>
    <handler
          class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:com.itemis.jbc.ui.commands.CreateClassFromJBCHandler"
          commandId="com.itemis.jbc.ui.commands.CreateClassFromJBCCommand">
    </handler>
    <handler
          class="com.itemis.jbc.ui.JBCExecutableExtensionFactory:com.itemis.jbc.ui.commands.OpenJBCFromJavaHandler"
          commandId="com.itemis.jbc.ui.commands.OpenJBCFromJavaCommand">
    </handler>
 </extension>
</plugin>
